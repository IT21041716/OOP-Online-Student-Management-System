<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         

   
         <servlet>
         	<servlet-name>Login</servlet-name>
         	<servlet-class>Com.user.loginServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>Login</servlet-name>
         	<url-pattern>/login</url-pattern>
         </servlet-mapping>
         
  
  		<servlet>
         	<servlet-name>UpdateAdmin</servlet-name>
         	<servlet-class>Com.user.AdminUpdateServlet</servlet-class>
        </servlet>
         
        <servlet-mapping>
        	<servlet-name>UpdateAdmin</servlet-name>
        	<url-pattern>/Update</url-pattern>
        </servlet-mapping>
 
 
	 		<servlet>
	         	<servlet-name>Registration</servlet-name>
	         	<servlet-class>Com.user.RegistrationServlet</servlet-class>
	        </servlet>
	         
	        <servlet-mapping>
	        	<servlet-name>Registration</servlet-name>
	        	<url-pattern>/insert</url-pattern>
	        </servlet-mapping>
	 
 
 			<servlet>
	         	<servlet-name>DeleteStudent</servlet-name>
	         	<servlet-class>Com.user.deleteStudentServlet</servlet-class>
	        </servlet>
	         
	        <servlet-mapping>
	        	<servlet-name>DeleteStudent</servlet-name>
	        	<url-pattern>/delete</url-pattern>
	        </servlet-mapping>
 
 		<servlet>
	         	<servlet-name>getdata</servlet-name>
	         	<servlet-class>Com.user.GetDataServlet</servlet-class>
	        </servlet>
	         
	        <servlet-mapping>
	        	<servlet-name>getdata</servlet-name>
	        	<url-pattern>/getdata</url-pattern>
	        </servlet-mapping>
 
 
 
 
 
 
 </web-app>